  "description": "npm-deep-dive examples",
  "main": "index.js",
  "scripts": {
    "prepare": "lefthook install",
    "pre-commit": "lint-staged",
    "prettier": "prettier '**/*.{json,yaml,yml}' --check",
    "prettier:fix": "prettier '**/*.{json,yaml,yml}' --write",
    "md": "markdownlint-cli2 '**/*.md' '#node_modules'",
    "md:fix": "markdownlint-cli2 --fix '**/*.md' '#node_modules'",
    "lint": "eslint '**/*.{js,jsx,ts,tsx}'",
    "lint:fix": "pnpm run lint --fix",
    "fix": "pnpm run prettier:fix && pnpm run md:fix && pnpm run lint:fix"
  },
  "keywords": [],
